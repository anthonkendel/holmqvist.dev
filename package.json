{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "eslint": "vue-cli-service lint",
    "eslint:fix": "vue-cli-service lint --fix",
    "test": "npm run eslint && npm run prettier:check"
  },
  "dependencies": {
    "core-js": "3.6.4",
    "vue": "2.6.11"
  },
  "devDependencies": {
    "@arbetsmyra/prettier-config": "1.0.0",
    "@vue/cli-plugin-babel": "4.3.1",
    "@vue/cli-plugin-typescript": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "dart-sass": "1.25.0",
    "prettier": "2.0.4",
    "sass-loader": "8.0.2",
    "typescript": "3.8.3",
    "vue-template-compiler": "2.6.11"
  },
  "renovate": {
    "extends": [
      "@arbetsmyra"
    ]
  },
  "prettier": "@arbetsmyra/prettier-config",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
